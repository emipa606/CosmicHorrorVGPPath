<Defs>
  <!-- Stirfry -->
  <RecipeDef ParentName="CookMealBase" Name="STR_Stirfrymeal">
    <defName>STR_Stirfrymeal</defName>
    <label>Cook four strange stir-fried meals</label>
    <description>Cooks a stir-fried meal from a combination of strange meat and raw plant ingredients. \n\nMakes enough for 4 meals.  \n\nFood Effects: Manipulation and Blood Filtration.</description>
    <jobString>Cooking strange stir-fried meals.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workAmount>1500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>ROM_StrangeMeatRaw</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
            <li>CookingSupplies</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>ROM_StrangeMeatRaw</li>
        <li>PlantFoodRaw</li>
        <li>CookingSupplies</li>
      </categories>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>ROM_StrangeMeatRaw</li>
        <li>PlantFoodRaw</li>
        <li>CookingSupplies</li>
      </categories>
      <disallowedCategories>
        <li>MeatRaw</li>
      </disallowedCategories>
      <disallowedThingDefs>
        <li>Meat_Human</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <products>
      <STR_stirfry>4</STR_stirfry>
    </products>
    <skillRequirements>
      <Cooking>10</Cooking>
    </skillRequirements>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
  </RecipeDef>
